# Maintainer: Your Name <your_email@example.com>

pkgname=trap
pkgver=1.0
pkgrel=1
pkgdesc="A tool for catching signals in C programs"
arch=('any')
url="https://example.com/trap"
license=('MIT')
depends=()
source=("trap")
sha256sums=('SKIP')

package() {
    mkdir -p "${pkgdir}/usr/bin"
    cp trap "${pkgdir}/usr/bin/"
}

